#PF-Exer-21
def check_registration_number(reg_number):
    temp=reg_number
    result_sum=0
    flag=True
    while(flag):
        while(temp>0):
            rem=temp%10
            result_sum+=rem
            temp=temp//10
        if(result_sum==9):
            result=True
            break
        elif(result_sum>9):
             result_sum=((result_sum%10)+(result_sum//10))
             if(result_sum==9):
                result=True
                break
             else:
                result=False
                break
        else:
            result=False
            break
    return result

print(check_registration_number(1043))
